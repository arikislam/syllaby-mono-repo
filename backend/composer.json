{
    "name": "laravel/laravel",
    "type": "project",
    "description": "The skeleton application for the Laravel framework.",
    "keywords": [
        "laravel",
        "framework"
    ],
    "license": "MIT",
    "require": {
        "php": "^8.3",
        "ext-dom": "*",
        "ext-libxml": "*",
        "aws/aws-sdk-php": "^3.314",
        "beeyev/disposable-email-filter-php": "^1.3",
        "creatomate/creatomate": "^1.1.0",
        "facebook/php-business-sdk": "^18.0",
        "google/apiclient": "^2.15",
        "google/cloud-pubsub": "^2.12",
        "guzzlehttp/guzzle": "^7.8.1",
        "intervention/image": "^2.7",
        "itsgoingd/clockwork": "^5.3",
        "landrok/language-detector": "^1.4",
        "laravel/cashier": "^15.0",
        "laravel/framework": "^11.0",
        "laravel/horizon": "^5.25",
        "laravel/pennant": "^1.11",
        "laravel/pulse": "^1.4",
        "laravel/sanctum": "^4.0",
        "laravel/scout": "^10.13",
        "laravel/slack-notification-channel": "^3.2",
        "laravel/socialite": "^5.14",
        "laravel/tinker": "^2.9",
        "league/flysystem-aws-s3-v3": "^3.28",
        "maennchen/zipstream-php": "^3.1",
        "openai-php/client": "^0.10.1",
        "php-ffmpeg/php-ffmpeg": "^1.1",
        "posthog/posthog-php": "^3.6",
        "remotion/lambda": "4.0.278",
        "rlanvin/php-rrule": "^2.5",
        "sentry/sentry-laravel": "^4.6.0",
        "socialiteproviders/threads": "^4.0",
        "socialiteproviders/tiktok": "^5.0",
        "socialiteproviders/youtube": "^4.1",
        "spatie/laravel-medialibrary": "^11.9",
        "spatie/laravel-query-builder": "^5.2",
        "spatie/simple-excel": "^3.7",
        "staudenmeir/laravel-adjacency-list": "^1.0",
        "stevebauman/purify": "^6.2",
        "stripe/stripe-php": "^13.0"
    },
    "require-dev": {
        "fakerphp/faker": "^1.9.1",
        "laravel/boost": "^1.0",
        "laravel/pail": "*",
        "laravel/pint": "^1.0",
        "laravel/sail": "^1.29.2",
        "mockery/mockery": "^1.6.12",
        "nunomaduro/collision": "^8.1",
        "pestphp/pest": "^2.34.8",
        "phpunit/phpunit": "^10.5.17"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        },
        "files": [
            "app/helper.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-update-cmd": [
            "@php artisan vendor:publish --tag=laravel-assets --ansi --force"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ]
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true,
            "php-http/discovery": true
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
